{
  "name": "xbell",
  "version": "0.3.2",
  "description": "A Web end-to-end testing framework.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "xbell": "./bin/run"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "start": "ts-node ./src/run.ts",
    "start:template-list": "ts-node ./src/run.ts --file template-list.spec.ts",
    "start:editor": "ts-node ./src/run.ts --file smart-detail-editor.spec.ts",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "end-to-end",
    "testing",
    "pom",
    "ioc",
    "typescript",
    "decorator",
    "multi-environment",
    "ddt"
  ],
  "author": "lianghang",
  "license": "ISC",
  "dependencies": {
    "@babel/code-frame": "^7.16.7",
    "chalk": "^4.1.2",
    "colors": "^1.4.0",
    "commander": "^9.2.0",
    "expect": "^28.1.0",
    "filenamify": "^4.3.0",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "pixelmatch": "^5.3.0",
    "playwright-core": "^1.22.0",
    "pngjs": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "stack-utils": "^2.0.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "xbell-reporter": "workspace:*"
  },
  "devDependencies": {
    "@types/babel__code-frame": "^7.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.33",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@types/reflect-metadata": "^0.1.0",
    "@types/stack-utils": "^2.0.1"
  }
}
