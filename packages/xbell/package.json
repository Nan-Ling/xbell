{
  "name": "xbell",
  "version": "0.4.1",
  "description": "A web testing framework.",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "xbell": "./bin/run.mjs"
  },
  "files": [
    "bin",
    "dist",
    "loader"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc && node ../../scripts/build.mjs",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./decorators": {
      "type": "./dist/decorators.d.ts",
      "import": "./dist/decorators.mjs"
    },
    "./loader/ignore-warning.cjs": {
      "require": "./loader/ignore-warning.cjs"
    },
    "./loader/loader.mjs": {
      "import": "./loader/loader.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index"
      ],
      "decorators": [
        "./dist/decorators/index"
      ]
    }
  },
  "keywords": [
    "end-to-end",
    "testing",
    "pom",
    "ioc",
    "typescript",
    "decorator",
    "multi-environment",
    "ddt"
  ],
  "author": {
    "name": "Hang Liang",
    "email": "xlianghang@163.com",
    "url": "https://github.com/xlianghang"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/code-frame": "^7.16.7",
    "@swc/core": "^1.2.208",
    "@vitejs/plugin-vue": "^3.0.3",
    "chalk": "^5.0.1",
    "commander": "^9.2.0",
    "debug": "^4.3.4",
    "expect": "^28.1.0",
    "expell": "workspace:*",
    "fast-glob": "^3.2.11",
    "filenamify": "^4.3.0",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "log-update": "^5.0.1",
    "pirates": "^4.0.5",
    "pixelmatch": "^5.3.0",
    "playwright-core": "^1.22.0",
    "pngjs": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "stack-utils": "^2.0.5",
    "strip-ansi": "^7.0.1",
    "vite": "^3.0.7",
    "vite-plugin-istanbul": "^2.9.0",
    "xbell-reporter": "workspace:*",
    "@xbell/code-stack": "workspace:*",
    "@xbell/color": "workspace:*"
  },
  "devDependencies": {
    "@types/babel__code-frame": "^7.0.3",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.182",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@types/reflect-metadata": "^0.1.0",
    "@types/stack-utils": "^2.0.1"
  }
}
